{
  "name": "ssh-keys-manager",
  "displayName": "SSH Keys Manager",
  "private": true,
  "version": "1.0.0",
  "description": "Tool to manage your SSH keys based on React and Electron",
  "main": "./src/main/index.js",
  "author": "Vittorio Scaperrotta",
  "license": "MIT",
  "engines": {
    "node": ">= 22.16.0",
    "npm": ">= 10.9.2"
  },
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "generate": "plop --plopfile=generators/index.js",
    "dev:client": "better-npm-run dev:client",
    "dev:server": "better-npm-run dev:server",
    "dev:storybook": "storybook dev -p 6006",
    "build": "better-npm-run build",
    "electron:serve": "better-npm-run electron:serve",
    "electron:build": "better-npm-run electron:build",
    "electron:start": "wait-on tcp:5173 && electron ."
  },
  "betterScripts": {
    "dev:client": {
      "command": "vite",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:server": {
      "command": "node ./src/server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "vite build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "electron:serve": {
      "command": "concurrently -k \"yarn dev:server\" \"cross-env BROWSER=none yarn dev:client\" \"yarn electron:start\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "electron:build": {
      "command": "electron-builder",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev:storybook": {
      "command": "storybook dev -p 6006",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "express": "^5.1.0",
    "feather-icons-react": "^0.9.0",
    "formik": "^2.4.6",
    "immer": "^10.1.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "storybook": "^8.5.0",
    "wait-on": "^8.0.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/manager-api": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/theming": "^8.5.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "better-npm-run": "^0.1.1",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.0",
    "electron": "^37.0.0",
    "electron-builder": "26.0.12",
    "plop": "^4.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "vite": "^6.0.7",
    "vite-plugin-eslint": "^1.8.1"
  }
}
